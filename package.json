{
  "name": "smart-bookmark",
  "displayName": "Smart Bookmark",
  "version": "0.0.1",
  "description": "AI-powered bookmark manager plugin",
  "author": "leonzeng",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.12.0",
    "idb": "^8.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.453.0",
    "plasmo": "0.89.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.27.0",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "bookmarks"
    ],
    "web_accessible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "tabs/app.html"
        ]
      }
    ]
  }
}
